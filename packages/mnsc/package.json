{
  "name": "@rutan/mnsc",
  "type": "module",
  "version": "0.3.0",
  "description": "",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/rutan/mnsc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rutan/mnsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s gen-parser build:*",
    "build:ts": "tsup src/index.ts --dts --format esm --sourcemap",
    "test": "run-s gen-parser && vitest run",
    "lint": "tsc --noEmit",
    "gen-parser": "run-p gen-parser:*",
    "gen-parser:mnsc": "peggy -o parser/mnsc-parser.js --format es parser/mnsc-parser.peggy",
    "gen-parser:frontmatter": "peggy -o parser/frontmatter-parser.js --format es parser/frontmatter-parser.peggy"
  },
  "devDependencies": {
    "peggy": "^5.0.6"
  }
}
